{
  "name": "colors",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/colors/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/colors",
        "index": "apps/colors/src/index.html",
        "main": "apps/colors/src/main.tsx",
        "tsConfig": "apps/colors/tsconfig.app.json",
        "assets": [
          "apps/colors/src/favicon.ico",
          "apps/colors/src/assets"
        ],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/colors/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/colors/src/environments/environment.ts",
              "with": "apps/colors/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "colors:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "colors:build:development"
        },
        "production": {
          "buildTarget": "colors:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
