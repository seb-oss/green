{
  "npmScope": "green",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "release": [{ "target": "release", "projects": "dependencies" }]
  },
  "projects": {
    "chlorophyll": {
      "tags": []
    },
    "extract": {
      "tags": []
    },
    "angular": {
      "tags": []
    },
    "angular-e2e": {
      "tags": [],
      "implicitDependencies": ["angular"]
    },
    "react": {
      "tags": []
    },
    "react-e2e": {
      "tags": [],
      "implicitDependencies": ["react"]
    }
  }
}
